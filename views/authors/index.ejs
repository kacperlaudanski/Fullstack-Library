<h2>All authors</h2>
<form action="/authors" method="GET">
    <label for="search-authors">Search authors</label>
    <input type="text" name="search-authors" id="search-authors" value="<%= searchOptions.name %>"/>
    <button type="submit">Search</button>
</form>
<% authors.forEach((author) => { %>
    <div><%= author.name %></div>
<% }) %>